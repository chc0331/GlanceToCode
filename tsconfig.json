{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "es6",
      "es2017"
    ],
    "strict": true,
    "types": [
      "@figma/plugin-typings"
    ],
    "outDir": ".",
    "skipLibCheck": true,
    "strictPropertyInitialization": false
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}